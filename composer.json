{
    "name": "shevaua/laravel-assets",
    "description": "Tools for manage assets",
    "require": {
        "php": "^7.0",
        "laravel/framework": "^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shevaua\\LaravelAssets\\Provider\\AssetProvider"
            ],
            "aliases": {
                "Assets": "Shevaua\\LaravelAssets\\Facade\\AssetGroupFacade"
            }
        }
    }
}
